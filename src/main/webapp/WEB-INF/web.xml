
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>HelloServlet</servlet-name>
		<servlet-class>org.dante.springboot.HelloServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FastjsonServlet</servlet-name>
		<servlet-class>org.dante.springboot.FastjsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MemServlet</servlet-name>
		<servlet-class>org.dante.springboot.MemServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloServlet</servlet-name>
		<url-pattern>/caas</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FastjsonServlet</servlet-name>
		<url-pattern>/json</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MemServlet</servlet-name>
		<url-pattern>/mem</url-pattern>
	</servlet-mapping>
</web-app>
