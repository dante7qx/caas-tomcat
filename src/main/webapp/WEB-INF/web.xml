<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>HelloServlet</servlet-name>
  	<display-name>HelloServlet</display-name>
  	<description></description>
  	<servlet-class>org.dante.springboot.HelloServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FastjsonServlet</servlet-name>
  	<display-name>FastjsonServlet</display-name>
  	<description></description>
  	<servlet-class>org.dante.springboot.FastjsonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloServlet</servlet-name>
  	<url-pattern>/caas</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FastjsonServlet</servlet-name>
  	<url-pattern>/json</url-pattern>
  </servlet-mapping>
</web-app>
